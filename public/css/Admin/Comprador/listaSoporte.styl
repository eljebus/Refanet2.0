@import 'styleAdmin'

body
	#contentContainer
		max-width 994px;
		margin auto !important
		padding-left 1em 
		padding-right 1em 
		margin-top 1em !important
		#lista
			font-family fuenteNormal
			text-align center
			.panel-heading
				padding-left 0
				padding-right 0
			ul 
				font-size 0
				text-align center
				li
					font-size 16px
			table tr
				text-align center
			table tr:nth-child(2n+2)
				background #F9FBFC
			
			.np
				width 9%
			.type,.status,.car
				width 15%
			.date
				width 15%
			.name
				width 46%
				text-align left
				padding-left 5px
				box-sizing border-box

		
