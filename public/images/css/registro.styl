	#registerForm
		margin-top -5px
		border-top solid 5px #15D4E5
		padding-top 2em
		padding-bottom 3em
		font-family: "Lato", Helvetica, Arial, sans-serif;
		form
			max-width 350px
			margin auto
		
		

